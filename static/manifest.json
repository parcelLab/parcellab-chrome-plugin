{
  "manifest_version": 3,
  "name": "parcelLab",
  "version": "0.5.1",
  "version_name": "0.5.1 beta",
  "description": "parcelLab Chrome plugin for Customer Service teams to retrieve and display trackings from parcelLab",
  "author": "jay@parcellab.com",
  "icons": {
    "16": "/img/parcelLab_Glyph_RGB_Color.png",
    "48": "/img/parcelLab_Glyph_RGB_Color.png",
    "128": "/img/parcelLab_Glyph_RGB_Color.png"
  },
  "action": {
    "default_icon": {
      "16": "/img/parcelLab_Glyph_RGB_Color.png",
      "24": "/img/parcelLab_Glyph_RGB_Color.png",
      "32": "/img/parcelLab_Glyph_RGB_Color.png"
    },
    "default_title": "parcelLab Order Tracking",
    "default_popup": "popup.html"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "options_page": "options.html",
  "permissions": ["storage", "sidePanel"],
  "host_permissions": ["https://api.parcellab.com/"],
  "homepage_url": "https://parcellab.com/",
  "minimum_chrome_version": "114"
}
